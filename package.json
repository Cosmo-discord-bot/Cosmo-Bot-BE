{
  "name": "diplomskabot",
  "version": "0.0.7",
  "description": "Multi server discord bot",
  "main": "main.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "swc src --out-dir build --sync & tsc --pretty --skipLibCheck --noEmit",
    "start": "node build/main.js",
    "start-pretty-pino": "node build/main.js | pino-pretty",
    "register-slash-commands": "node build/slash-commands/registering-slash-commands.js | pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J4ncHy/diplomskabot.git"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "bot"
  ],
  "author": "Jan Mrak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/J4ncHy/diplomskabot/issues"
  },
  "homepage": "https://github.com/J4ncHy/diplomskabot#readme",
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.2",
    "mongoose": "^7.2.2",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8"
  }
}
